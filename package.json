{
  "name": "RNDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint --ext .js ./src --fix",
    "svg-to-xml": "cd src/tools && node ./svgToXml.js",
    "dev-menu": "adb shell input keyevent 82",
    "devtools": "react-devtools",
    "dev-android": "react-native run-android",
    "dev-ios": "react-native run-ios",
    "build": "cd android && gradlew assembleRelease",
    "test-build": "cd android && gradlew installRelease",
    "test-install": "adb install ./android/app/build/outputs/apk/app-debug.apk",
    "clear-build-cache": "cd android && gradlew clean",
    "clear": "rimraf android/app/build && npm run clear-build-cache",
    "clear-all": "yarn cache clean -f && npm cache clean -f && cnpm cache clean -f && npm run clear",
    "gradle-debug": "cd android && gradlew compiledebug --debug",
    "gradle-info": "cd android && gradlew compiledebug --info",
    "gradle-stacktrace": "cd android && gradlew compiledebug --stacktrace"
  },
  "dependencies": {
    "antd-mobile-rn": "^2.2.0",
    "immutable": "^3.8.2",
    "md5": "^2.2.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-draftjs-render": "^2.8.2",
    "react-native-i18n": "^2.0.15",
    "react-native-image-crop-picker": "^0.20.3",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-splash-screen": "3.0.9",
    "react-native-storage": "^0.2.2",
    "react-native-svg": "^6.5.1",
    "react-native-svg-uri": "^1.2.3",
    "react-native-vector-icons": "^4.6.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-jest": "22.4.4",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "jest": "22.4.4",
    "node-sass": "^4.9.2",
    "react-native-sass-transformer": "^1.2.2",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
